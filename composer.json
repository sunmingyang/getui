{
    "name": "haixin/getui",
    "description": "getui for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "HaiXin",
            "email": "nirvana488939@gmail.com",
            "homepage": "https://github.com/sunmingyang/getui"
        }
    ],
    "homepage": "https://github.com/sunmingyang/getui",
    "keywords": ["Laravel", "GeTui"],
    "require": {
        "illuminate/support": "^5.5|~6.0|~7.0|~8.0",
        "guzzlehttp/guzzle": "^6.2 || ^7.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "HaiXin\\GeTui\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HaiXin\\GeTui\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HaiXin\\GeTui\\GeTuiServiceProvider"
            ],
            "aliases": {
                "GeTui": "HaiXin\\GeTui\\Facades\\GeTui"
            }
        }
    },
        "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
